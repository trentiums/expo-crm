    "rm:nm": "yarn cache clean && npm cache verify && npm cache clean --force && rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && rm -rf android && rm -rf ios",
    "ios:clean": "cd ios && pod deintegrate && rm -rf build && rm -rf Podfile.lock && pod install && cd ..",
